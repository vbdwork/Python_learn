full_price = int(input())
price_1 = int(input())
price_2 = int(input())
summa = full_weight * full_price
weight_1 = (summa - (price_2 * full_weight)) / (price_1 - price_2)
weight_2 = full_weight - weight_1
print(int(weight_1), int(weight_2))
